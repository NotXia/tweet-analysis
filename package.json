{
  "name": "tweet-analysis",
  "version": "0.0.1",
  "description": "Non ho mandato l'email",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "npm run test:unit && npm run test:api && npm run test:react",
    "test:unit": "jest --passWithNoTests --coverage --coverageDirectory ./coverage/unit --globalSetup ./tests/utils/unit.global_init.js ./tests/unit/.*.test.js",
    "test:api": "jest --passWithNoTests --coverage --coverageDirectory ./coverage/api --globalSetup ./tests/utils/api.global_init.js ./tests/api/.*.test.js",
    "test:react": "jest --passWithNoTests --coverage --coverageDirectory ./coverage/react --globalSetup ./tests/utils/react.global_init.js ./frontend/src/.*.test.jsx --env=jsdom",
    "build": "npm run build --prefix ./frontend"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "./tests/utils/"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/tests/utils/style_mock.js"
    }
  },
  "dependencies": {
    "axios": "^1.1.3",
    "bootstrap": "^5.2.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "country-iso-2-to-3": "^1.1.0",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "http-errors": "^2.0.0",
    "jquery": "^3.6.1",
    "languagedetect": "^2.0.0",
    "moment": "^2.29.4",
    "node-nlp": "^4.24.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.4.2",
    "react-scripts": "5.0.1",
    "sentiment-multilang": "^0.0.2",
    "stopword": "^2.0.5",
    "stopwords-iso": "^1.1.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "babel-jest": "^29.2.2",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "nock": "^13.2.9",
    "react-test-renderer": "^18.2.0",
    "supertest-session": "^4.1.0"
  }
}
