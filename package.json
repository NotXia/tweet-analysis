{
  "name": "tweet-analysis",
  "version": "0.0.1",
  "description": "Non ho mandato l'email",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "npm run test:unit && npm run test:api",
    "test:unit": "jest --passWithNoTests --coverage --coverageDirectory ./coverage/unit --globalSetup ./tests/utils/unit.global_init.js ./tests/unit/.*.test.js",
    "test:api": "jest --passWithNoTests --coverage --coverageDirectory ./coverage/api --globalSetup ./tests/utils/api.global_init.js ./tests/api/.*.test.js",
    "build": "npm run build --prefix ./frontend"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "./tests/utils/"
    ]
  },
  "dependencies": {
    "bootstrap": "^5.2.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "http-errors": "^2.0.0",
    "languagedetect": "^2.0.0",
    "node-nlp": "^4.24.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.4.2",
    "react-scripts": "5.0.1",
    "sentiment-multilang": "^0.0.2",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "jest": "^29.2.1",
    "supertest-session": "^4.1.0"
  }
}
